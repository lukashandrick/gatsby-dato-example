{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Gatsby develop",
            "type": "pwa-node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/.bin/gatsby",
            "args": [
                "develop"
            ],
            "runtimeArgs": [],
            "console": "integratedTerminal",
            "localRoot": "${workspaceRoot}/",
            "runtimeVersion": "18.12.1"
        },
        {
            "name": "Gatsby build",
            "type": "pwa-node",
            "request": "launch",
            "program": "~/node_modules/.bin/gatsby",
            "args": [
                "build", "--verbose"
            ],
            "runtimeArgs": [],
            "console": "integratedTerminal",
            "localRoot": "${workspaceRoot}/",
            "runtimeVersion": "18.12.1"
        }
    ]
}